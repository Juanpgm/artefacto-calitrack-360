{
  "name": "calitrack-360",
  "version": "1.0.0",
  "description": "PWA para captura de informaciÃ³n de proyectos de infraestructura",
  "private": true,
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "setup": "npm run setup:windows || npm run setup:unix",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File ./setup.ps1",
    "setup:unix": "bash ./setup.sh",
    "verify": "npm run verify:windows || npm run verify:unix",
    "verify:windows": "powershell -ExecutionPolicy Bypass -File ./verify-setup.ps1",
    "verify:unix": "bash ./verify-setup.sh",
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build",
    "preview": "cd frontend && npm run preview",
    "install:all": "cd frontend && npm install"
  },
  "keywords": [
    "pwa",
    "svelte",
    "firebase",
    "geolocation",
    "camera"
  ],
